<resources xmlns:android="http://schemas.android.com/apk/res/android">    <style name="AppBaseTheme" parent="android:Theme.DeviceDefault.Light.NoActionBar">        <!--        @android:style/Theme.DeviceDefault.Dialog.NoActionBar            android:style/Theme.DeviceDefault.Light.NoActionBarcan go in            res/values-vXX/styles.xml, while customizations related to            backward-compatibility can go here.-->    </style>    <!-- Application theme. -->    <style name="AppTheme" parent="AppBaseTheme">        <!-- All customizations that are NOT specific to a particular API-level can go here. -->    </style>    <!--设置避免启动界面时的黑屏-->    <style name="Theme.AppStartLoad" parent="android:Theme">        <item name="android:windowBackground">@drawable/bg_welcome</item>        <item name="android:windowNoTitle">true</item>    </style>    <style name="head_icon">        <item name="android:width">@dimen/head_icon_w</item>        <item name="android:height">@dimen/head_icon_h</item>        <item name="android:scaleType">fitCenter</item>    </style>    <style name="bottom_iconbtn">        <item name="android:height">@dimen/bottom_h</item>        <item name="android:padding">@dimen/bottom_btn_padding</item>        <item name="android:gravity">center_horizontal</item>        <item name="android:textSize">@dimen/bottom_btn_textsize</item>        <item name="android:textColor">@color/bottom_btn_color</item>        <item name="android:background">@drawable/bottom_btn_bg</item>    </style>    <!-- form -->    <style name="form_singleline">        <item name="android:background">@color/white</item>        <item name="android:gravity">center_vertical</item>        <item name="android:paddingLeft">@dimen/form_singleline_padding</item>        <item name="android:paddingRight">@dimen/form_singleline_padding</item>    </style>    <style name="form_singleline_te">        <item name="android:background">@null</item>        <item name="android:singleLine">true</item>        <item name="android:padding">0dp</item>    </style>    <style name="form_line_te">        <item name="android:background">@null</item>        <item name="android:padding">0dp</item>    </style>    <style name="menu_labels_style">        <item name="android:background">@drawable/fab_label_background</item>        <item name="android:textColor">#FFFFFF</item>    </style>    <style name="ActionSheetDialogAnimation" parent="@android:style/Animation.Dialog">        <item name="android:windowEnterAnimation">@anim/bottom_anim_dialog_in</item>        <item name="android:windowExitAnimation">@anim/bottom_anim_dialog_out</item>    </style>    <style name="BottomAnimDialogStyle" parent="@android:style/Theme.Dialog">        <item name="android:windowFrame">@null</item><!--边框-->        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->        <item name="android:windowNoTitle">true</item><!--无标题-->        <item name="android:windowContentOverlay">@null</item>        <item name="android:backgroundDimEnabled">true</item>        <item name="android:windowIsTranslucent">false</item>        <item name="android:windowBackground">@color/transparent_background</item><!--背景透明-->        <item name="android:background">@color/transparent_background</item>        <item name="android:backgroundDimAmount">0.5</item>        <item name="android:windowAnimationStyle">@style/ActionSheetDialogAnimation</item><!--动画-->    </style></resources>